    "## 2. Interactive Plots with Plotly <a id='plotly'></a>"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Interactive spectrum viewer\n",
    "fig = go.Figure()\n",
    "\n",
    "# Add traces for each spectrum\n",
    "for isotope, data in spectra.items():\n",
    "    fig.add_trace(go.Scatter(\n",
    "        x=data['channels'],\n",
    "        y=data['counts'],\n",
    "        mode='lines',\n",
    "        name=isotope,\n",
    "        line=dict(width=1),\n",
    "        hovertemplate='Channel: %{x}<br>Counts: %{y}<extra></extra>'\n",
    "    ))\n",
    "    \n",
    "    # Add peak markers\n",
    "    for peak in data['peaks']:\n",
    "        fig.add_trace(go.Scatter(\n",
    "            x=[peak['centroid']],\n",
    "            y=[peak['amplitude'] + peak.get('bg_intercept', 0)],\n",
    "            mode='markers',\n",
    "            marker=dict(size=10, symbol='diamond'),\n",
    "            name=f\"{isotope} peak\",\n",
    "            showlegend=False,\n",
    "            hovertemplate=f'Peak: {peak[\"centroid\"]:.1f}<br>Area: {peak[\"area\"]:.0f}<extra></extra>'\n",
    "        ))\n",
    "\n",
    "# Update layout\n",
    "fig.update_layout(\n",
    "    title='Interactive Gamma Spectra Comparison',\n",
    "    xaxis_title='Channel',\n",
    "    yaxis_title='Counts',\n",
    "    yaxis_type='log',\n",
    "    hovermode='x unified',\n",
    "    height=600,\n",
    "    template='plotly_white'\n",
    ")\n",
    "\n",
    "# Add range slider\n",
    "fig.update_xaxes(rangeslider_visible=True)\n",
    "\n",
    "fig.show()"
