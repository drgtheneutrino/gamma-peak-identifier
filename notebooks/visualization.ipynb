   "source": [
    "# Contour plot of fitting residuals\n",
    "data = spectra['Na-22']\n",
    "peak = data['peaks'][0]\n",
    "start, end = peak['fit_region']\n",
    "\n",
    "# Create parameter grid\n",
    "n_points = 50\n",
    "centroid_range = np.linspace(peak['centroid'] - 5, peak['centroid'] + 5, n_points)\n",
    "sigma_range = np.linspace(peak['sigma'] * 0.5, peak['sigma'] * 1.5, n_points)\n",
    "\n",
    "# Calculate chi-square for each parameter combination\n",
    "chi_square = np.zeros((n_points, n_points))\n",
    "x_fit = data['channels'][start:end]\n",
    "y_data = data['counts'][start:end]\n",
    "\n",
    "for i, cent in enumerate(centroid_range):\n",
    "    for j, sig in enumerate(sigma_range):\n",
    "        # Calculate model with varied parameters\n",
    "        if peak['fit_params'] is not None:\n",
    "            params = peak['fit_params'].copy()\n",
    "            params[1] = cent\n",
    "            params[2] = sig\n",
    "            y_model = gaussian_with_background(x_fit, *params)\n",
    "            \n",
    "            # Calculate chi-square\n",
    "            residuals = (y_data - y_model) / np.sqrt(np.maximum(y_data, 1))\n",
    "            chi_square[j, i] = np.sum(residuals**2) / len(residuals)\n",
    "\n",
    "# Create contour plot\n",
    "fig = go.Figure(data=go.Contour(\n",
    "    x=centroid_range,\n",
    "    y=sigma_range,\n",
    "    z=chi_square,\n",
    "    colorscale='Viridis',\n",
    "    showscale=True,\n",
    "    colorbar=dict(title='χ² / ndf'),\n",
    "    contours=dict(\n",
    "        start=chi_square.min(),\n",
    "        end=chi_square.min() + 10,\n",
    "        size=0.5\n",
    "    )\n",
    "))\n",
    "\n",
    "# Add best fit point\n",
    "fig.add_trace(go.Scatter(\n",
    "    x=[peak['centroid']],\n",
    "    y=[peak['sigma']],\n",
    "    mode='markers',\n",
    "    marker=dict(size=10, color='red', symbol='x'),\n",
    "    name='Best Fit'\n",
    "))\n",
    "\n",
    "fig.update_layout(\n",
    "    title='Chi-Square Landscape for Peak Fitting',\n",
    "    xaxis_title='Centroid (channels)',\n",
    "    yaxis_title='Sigma (channels)',\n",
    "    height=500\n",
    ")\n",
    "\n",
    "fig.show()"
   ]
