   "source": [
    "# Generate sample data\n",
    "np.random.seed(42)\n",
    "\n",
    "# Create multiple spectra for comparison\n",
    "spectra = {}\n",
    "isotopes = ['Na-22', 'Co-60', 'Cs-137']\n",
    "peak_data = {\n",
    "    'Na-22': [(511, 2000, 6), (1274, 1500, 9)],\n",
    "    'Co-60': [(1173, 1800, 8), (1332, 1600, 9)],\n",
    "    'Cs-137': [(661, 2500, 7)]\n",
    "}\n",
    "\n",
    "for isotope in isotopes:\n",
    "    channels, counts = generate_synthetic_spectrum(\n",
    "        num_channels=2048,\n",
    "        peaks=peak_data[isotope],\n",
    "        background_level=30,\n",
    "        noise_level=1\n",
    "    )\n",
    "    \n",
    "    # Detect and fit peaks\n",
    "    peaks = detect_peaks(counts, min_prominence=50)\n",
    "    fitted = fit_peaks(channels, counts, peaks)\n",
    "    \n",
    "    spectra[isotope] = {\n",
    "        'channels': channels,\n",
    "        'counts': counts,\n",
    "        'peaks': fitted\n",
    "    }\n",
    "    \n",
    "print(f\"Generated {len(spectra)} test spectra\")"
